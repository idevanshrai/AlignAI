{
  "version": 2,
  "routes": [
    { "src": "^/css/(.*)", "dest": "/css/$1" },
    { "src": "^/js/(.*)", "dest": "/js/$1" },
    { "handle": "filesystem" },
    { "src": "/$", "dest": "/splash.html" },
    { "src": "/(.*)\\.html", "dest": "/$1.html" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
} 