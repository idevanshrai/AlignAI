{
  "version": 2,
  "routes": [
    { "src": "^/css/(.*)", "dest": "/css/$1" },
    { "src": "^/js/(.*)", "dest": "/js/$1" },
    { "handle": "filesystem" },
    { "src": "^/$", "dest": "/splash.html" },
    { "src": "^/index$", "dest": "/splash.html" },
    { "src": "^/index.html$", "dest": "/splash.html" },
    { "src": "^/(login|splash|index)/?$", "dest": "/$1.html" }
  ]
} 